      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "source.c storage.modifier"
      ],
      "settings": {
        "foreground": "#006000"
      }
    },
    {
      "scope": [
        "source.c storage.type"
      ],
      "settings": {
        "foreground": "#006000"
      }
    },
    {
      "scope": [
