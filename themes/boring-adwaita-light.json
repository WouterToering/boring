{
  "$schema": "vscode://schemas/color-theme",
  "name": "boring adwaita light",
  "type": "light",
  "include": "./light-base.json",
  "tokenColors": [
    {
      "scope": [
        "",
        "meta.embedded",
        "variable",
        "meta.tag.attributes punctuation.section.embedded",
        "keyword.operator",
        "storage.type.function.arrow",
        "keyword.control.flow.block-scalar.literal",
        "keyword.control.flow.block-scalar.folded",
        "storage.modifier.chomping-indicator",
        "storage.type.string",
        "string.quoted.byte.raw",
        "meta.macro.rules entity.name.function.macro.rust"
      ],
      "settings": {
        "foreground": "#37363a", // 504E55
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.numeric.binary",
        "constant.numeric.octal",
        "constant.numeric.hex",
        "keyword.other.unit.binary",
        "keyword.other.unit.octal",
        "keyword.other.unit.hexadecimal",
        "keyword.other.unit.imaginary",
        "keyword.other.unit.exponent"
      ],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.language.boolean", "constant.language.bool"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["comment", "entity.other.document.begin.yaml", "entity.other.document.end.yaml"],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.language",
        "string.quoted.single.char",
        "support.type.property-name",
        "support.constant.property-value.css",
        "source.css keyword.other.unit"
      ],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric", "constant.numeric entity.name.type.numeric"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#bbb",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.numeric.float"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "#1C71D8",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.heading.markdown"],
      "settings": {
        "foreground": "#006000"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.operator.new",
        "keyword.operator.logical.python",
        "source.js keyword.operator.expression",
        "source.ts keyword.operator.expression",
        "storage.modifier",
        "storage.type.class",
        "storage.type.function",
        "entity.name.tag.yaml",
        "source.js storage.type",
        "source.ts storage.type",
        "source.tsx storage.type",
        "source.rust storage.type"
      ],
      "settings": {
        "foreground": "#0052b1" // 0052b1
      }
    },
    {
      "scope": ["constant.numeric", "keyword.operator.address.go"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": [],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "meta.preprocessor keyword.control",
        "punctuation.decorator",
        "meta.decorator entity.name.function",
        "entity.name.function.decorator",
        "keyword.control.at-rule.media",
        "constant.character.entity",
        "punctuation.section.embedded",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#0052b1",
        "fontStyle": ""
      }
    },
    {
      "scope": ["comment.line.number-sign.shebang"],
      "settings": {
        "foreground": "#bbb"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ED333B",
        "fontStyle": ""
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#006000",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "storage.type",
        "entity.name.type",
        "entity.name.namespace",
        "keyword.type.cs",
        "support.type",
        "support.class.builtin",
        "support.class.promise"
      ],
      "settings": {
        "foreground": "#006000"
      }
    },
    {
      "scope": ["meta.preprocessor.cs"],
      "settings": {
        "foreground": "#5E5C64",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.other.placeholder"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["source.c storage.modifier"],
      "settings": {
        "foreground": "#006000"
      }
    },
    {
      "scope": ["source.c storage.type"],
      "settings": {
        "foreground": "#006000"
      }
    },
    {
      "scope": ["entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#006000"
      }
    },
    {
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#0052b1",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-class.css",
        "meta.selector.css punctuation.section.function"
      ],
      "settings": {
        "foreground": "#ad5c00"
      }
    },
    {
      "scope": ["meta.selector.css keyword.operator", "entity.other.attribute-name.css"],
      "settings": {
        "foreground": "#0052b1"
      }
    },
    {
      "scope": ["support.type.vendored.property-name.css"],
      "settings": {
        "foreground": "#D38B09",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.inserted.diff"],
      "settings": {
        "foreground": "#26A1A2",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#E66100",
        "fontStyle": ""
      }
    },
    {
      "scope": ["meta.diff.header"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["meta.diff.range"],
      "settings": {
        "foreground": "#D38B09",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.deleted.diff"],
      "settings": {
        "foreground": "#F66151",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.other.placeholder.go"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["support.function.builtin.python"],
      "settings": {
        "foreground": "#1C71D8",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.type.class.python"],
      "settings": {
        "foreground": "#006000"
      }
    },
    {
      "scope": ["keyword.control.import.python"],
      "settings": {
        "foreground": "#E01B24",
        "fontStyle": ""
      }
    },
    {
      "scope": ["meta.attribute.rust", "meta.attribute.rust keyword.operator"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.type.lifetime.rust"],
      "settings": {
        "foreground": "#0052b1",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.function.macro"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "meta.tag entity.other.attribute-name",
        "meta.tag keyword.operator.assignment",
        "punctuation.separator.key-value.html",
        "punctuation.separator.key-value.svelte",
        "text.xml meta.tag"
      ],
      "settings": {
        "foreground": "#0052b1",
        "fontStyle": ""
      }
    },
    {
      "scope": ["meta.tag string"],
      "settings": {
        "foreground": "#ad5c00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component.svelte", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#006000",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "text.xml meta.tag.preprocessor entity.name.tag",
        "text.xml meta.tag.preprocessor punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#D38B09"
      }
    },
    {
      "scope": ["markup.italic.markdown"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.strikethrough.markdown"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    }
  ]
}
