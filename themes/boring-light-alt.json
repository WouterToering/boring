{
  "$schema": "vscode://schemas/color-theme",
  "name": "boring light alt",
  "type": "light",
  "include": "./light-base.json",
  // "semanticHighlighting": true,
  "colors": {
    "tab.activeBackground": "#fbfbfb",
    "tab.activeBorder": "#fbfbfb",
    "tab.hoverBackground": "#fbfbfb",
    "editor.background": "#fbfbfb",
    "editor.foreground": "#24292E"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#8265c0"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "entity.name.import.go"],
      "settings": {
        "foreground": "#28916B"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        // "entity.name.function",
        "support.function",
        // "support.constant.handlebars",
        // "source.powershell variable.other.member",
        // "meta.function-call.generic.python"
        "entity.name",
        "meta.function-call.generic"
      ],
      "settings": {
        // "foreground": "#07a"
        "foreground": "#0062a8"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#B2503C"
      }
    },
    {
      "name": "Control flow",
      "scope": ["keyword.control", "storage.type.function", "keyword.function"],
      "settings": {
        "foreground": "#5c6975" // 515c67
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#000000"
      }
    }
  ]
}
