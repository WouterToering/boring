{
  "$schema": "vscode://schemas/color-theme",
  "name": "boring light alt",
  "type": "light",
  "include": "./light-base.json",
  "colors": {
    "editor.foreground": "#1c1c1c",
    "editor.background": "#f8f8fa",

    "terminal.foreground": "#3C3836",
    "terminal.ansiBlack": "#24292f",
    "terminal.ansiRed": "#9D0006",
    "terminal.ansiGreen": "#427B58",
    "terminal.ansiYellow": "#b07619",
    "terminal.ansiBlue": "#005c9d",
    "terminal.ansiMagenta": "#8250df",
    "terminal.ansiCyan": "#1b7c83",
    "terminal.ansiWhite": "#6e7781",
    "terminal.ansiBrightBlack": "#24292f",
    "terminal.ansiBrightRed": "#9D0006",
    "terminal.ansiBrightGreen": "#427B58",
    "terminal.ansiBrightYellow": "#B57614",
    "terminal.ansiBrightBlue": "#005c9d",
    "terminal.ansiBrightMagenta": "#8250df",
    "terminal.ansiBrightCyan": "#1b7c83",
    "terminal.ansiBrightWhite": "#6e7781"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#888" // 7c43b1
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "entity.name.import.go"],
      "settings": {
        "foreground": "#006b42" // 004B20
      }
    },
    {
      "name": "Function declarations",
      "scope": ["support.function", "entity.name", "meta.function-call.generic"],
      "settings": {
        "foreground": "#00666b" // 5F819D
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language", "keyword.other.unit.hexadecimal"],
      "settings": {
        "foreground": "#00406b"
      }
    },
    {
      "name": "Control flow",
      "scope": [
        "keyword.control",
        "storage.type.function",
        "keyword.function",
        // "keyword.operator",
        "keyword.type",
        "keyword.var",
        "keyword.struct",
        "storage.type.class.python"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#000000"
      }
    }
  ]
}
