{
  "$schema": "vscode://schemas/color-theme",
  "name": "boring light",
  "type": "light",
  "include": "./light-base.json",
  "colors": {
    "editor.foreground": "#1C1C1C"
  },
  "tokenColors": [
    {
      "name": "Control flow",
      "scope": [
        "keyword.control",
        "keyword.operator.logical",
        "storage.type.function",
        "keyword.function",
        "keyword.type",
        "keyword.var",
        "keyword.struct",
        "storage.type.class.python"
      ],
      "settings": {
        "foreground": "#5e5e5e"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function.go",
        "entity.name.function.support",
        "meta.function-call.generic",
        "entity.name.function.decorator.python",
        "support.type.python"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "entity.name.import.go", "punctuation.definition.string"],
      "settings": {
        "foreground": "#4a4a4a"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language", "keyword.other.unit.hexadecimal"],
      "settings": {
        "foreground": "#373737"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "scope": ["punctuation", "keyword.operator"],
      "settings": {}
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json", "punctuation.support.type.property-name"],
      "settings": {
        "foreground": "#24292E"
      }
    }
  ]
}
