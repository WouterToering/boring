{
  "$schema": "vscode://schemas/color-theme",
  "name": "boring light",
  "type": "light",
  "include": "./light-base.json",
  // "semanticHighlighting": true,
  "colors": {
    "tab.activeBackground": "#F7F7F7", // F7F7F7
    "tab.activeBorder": "#F7F7F7",
    "tab.hoverBackground": "#F7F7F7",
    "editor.lineHighlightBackground": "#fcfcfc",
    "editor.background": "#F7F7F7",
    "editor.foreground": "#1b1b1b" // 24292E
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#888" // 006c8a
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "entity.name.import.go"],
      "settings": {
        "foreground": "#007936"
      }
    },
    {
      "name": "Function declarations",
      "scope": ["support.function", "entity.name", "meta.function-call.generic"],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#d30038"
      }
    },
    {
      "name": "Control flow",
      "scope": [
        "keyword.control",
        "storage.type.function",
        "keyword.function",
        "keyword.operator",
        "keyword.type",
        "keyword.var",
        "keyword.struct",
        "storage.type.class.python"
      ],
      "settings": {
        "foreground": "#005299" // 515c67
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#000000"
      }
    }
  ]
}
