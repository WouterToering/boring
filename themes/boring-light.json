{
  "$schema": "vscode://schemas/color-theme",
  "name": "boring light",
  "type": "light",
  "include": "./light-base.json",
  "colors": {
    "editor.foreground": "#353535",
    "editor.background": "#fdfcfc",

    "terminal.foreground": "#353535",
    "terminal.ansiBlack": "#24292f",
    "terminal.ansiRed": "#842828",
    "terminal.ansiGreen": "#628562",
    "terminal.ansiYellow": "#846128",
    "terminal.ansiBlue": "#286A84",
    "terminal.ansiMagenta": "#8250df",
    "terminal.ansiCyan": "#1b7c83",
    "terminal.ansiWhite": "#6e7781",
    "terminal.ansiBrightBlack": "#24292f",
    "terminal.ansiBrightRed": "#842828",
    "terminal.ansiBrightGreen": "#628562",
    "terminal.ansiBrightYellow": "#846128",
    "terminal.ansiBrightBlue": "#286A84",
    "terminal.ansiBrightMagenta": "#8250df",
    "terminal.ansiBrightCyan": "#1b7c83",
    "terminal.ansiBrightWhite": "#6e7781"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#9d9180" // A89375
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "entity.name.import.go"],
      "settings": {
        "foreground": "#628562" // 004B20
      }
    },
    {
      "name": "Functions",
      "scope": ["support.function", "entity.name", "meta.function-call.generic"],
      "settings": {
        // "foreground": "#006868"
        // "fontStyle": "bold",
        "foreground": "#000"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language", "keyword.other.unit.hexadecimal"],
      "settings": {
        "foreground": "#842828"
      }
    },
    {
      "name": "Control flow",
      "scope": [
        "keyword.control",
        "storage.type.function",
        "keyword.function",
        // "keyword.operator",
        "keyword.type",
        "keyword.var",
        "keyword.struct",
        "storage.type.class.python"
      ],
      "settings": {
        "foreground": "#286A84"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#000000"
      }
    }
  ]
}
