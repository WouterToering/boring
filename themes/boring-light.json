{
  "$schema": "vscode://schemas/color-theme",
  "name": "boring light",
  "type": "light",
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#888"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "entity.name.import.go", "punctuation.definition.string"],
      "settings": {
        "foreground": "#1E766D"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language", "keyword.other.unit.hexadecimal"],
      "settings": {
        "foreground": "#026B6B" // 1480ad
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function.go",
        "entity.name.function.support",
        "meta.function-call.generic",
        "entity.name.function.decorator.python",
        "support.type.python"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Control flow",
      "scope": [
        "keyword.control",
        "keyword.operator.logical",
        "storage.type.function",
        "keyword.function",
        "keyword.type",
        "keyword.var",
        "keyword.struct",
        "storage.type.class.python"
      ],
      "settings": {
        "foreground": "#1D699F"
      }
    },
    {
      "scope": ["punctuation", "keyword.operator"],
      "settings": {
        // "foreground": "#444"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json",
        "punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#080808"
      }
    }
  ],
