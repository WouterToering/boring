{
  "$schema": "vscode://schemas/color-theme",
  "name": "boring light",
  "type": "light",
  "include": "./light-base.json",
  "colors": {
    "editor.foreground": "#000"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring.multi"],
      "settings": {
        "foreground": "#947D0F" // 7c43b1
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "entity.name.import.go"],
      "settings": {
        "foreground": "#0C751A"
      }
    },
    {
      "name": "Function declarations",
      "scope": ["support.function", "entit2y.name", "meta.function-call.generic"],
      "settings": {
        // "foreground": "#398EB1",
        "foreground": "#00576E"
        // "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language", "keyword.other.unit.hexadecimal"],
      "settings": {
        "foreground": "#1646E8"
      }
    },
    {
      "name": "Control flow",
      "scope": [
        "keyword.control",
        "storage.type.function",
        "keyword.function",
        "keyword.operator",
        "keyword.type",
        "keyword.var",
        "keyword.struct",
        "storage.type.class.python"
      ],
      "settings": {
        "foreground": "#002DAA" // 515c67 - 155b84
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#000000"
      }
    }
  ]
}
